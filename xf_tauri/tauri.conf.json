{
    "productName": "xf_tauri",
    "version": "0.1.0",
    "identifier": "com.braid.xf-tauri",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:1420",
        "frontendDist": "./dist"
    },
    "app": {
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "LocalLink",
                "width": 800,
                "decorations": false
            }
        ],
        "security": {
            "csp": null,
            "capabilities": [
                "default"
            ]
        },
        "withGlobalTauri": true
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "plugins": {}
}